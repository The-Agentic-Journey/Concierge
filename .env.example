# Server
PORT=8080
AUTH_TOKEN=your-secret-token-here

# Scalebox
SCALEBOX_API_URL=https://your-scalebox.example.com
SCALEBOX_API_TOKEN=your-scalebox-token
SCALEBOX_TEMPLATE=agentic-0-authenticated
SCALEBOX_HOST=your-scalebox.example.com

# SSH (optional - defaults to scalebox)
SSH_USER=scalebox

# Knowledge Repository (data only - scripts are bundled in container)
KNOWLEDGE_REPO_URL=git@github.com:your/knowledge-repo.git
KNOWLEDGE_REPO_BRANCH=main

# Execution (default: 20 min)
EXECUTION_TIMEOUT_MS=1200000
